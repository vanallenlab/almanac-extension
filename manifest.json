{
	"name": "Molecular Oncology Almanac Connector",
    "short_name": "MOA Connector",
	"manifest_version": 2,
	"version": "2.00",
	"description": "Submit alteration-action relationships to the Molecular Oncology Almanac from Chrome",
    "default_locale": "en",
	"browser_action": {
		"default_icon": {
			"16": "cs_horz_16.png",
			"32": "cs_horz_32.png",
			"48": "cs_horz_48.png",
			"128": "cs_horz_128.png"
		},
		"default_title": "Submit assertion for review",
		"default_popup": "popup.html"
	},
	"permissions": ["https://moalmanac.org/*"],
	"background": {
		"scripts": [
			"static/js/jquery-1.11.2.js",
			"static/js/select2.full.min.js",
			"static/js/portal-init-datatable.js",
			"portal-json.js",
			"portal-add.js"
		]
	},

	"content_security_policy": "script-src 'self'; object-src 'self'",
	"homepage_url": "https://moalmanac.org/",
	"icons": {
		"48": "cs_horz_48.png"
	},
	"minimum_chrome_version": "55"
}